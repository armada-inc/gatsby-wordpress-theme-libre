{
  "name": "gatsby-wordpress-theme-workspace",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "develop": "yarn workspace try-ghost develop",
    "build": "yarn workspace try-ghost build",
    "clean": "yarn workspace try-ghost clean",
    "serve": "yarn workspace try-ghost serve",
    "bootstrap": "lerna bootstrap --use-workspaces",
    "pack": "lerna run build --scope={$(ls ./packages | xargs echo | sed 's/ /,/g')}"
  },
  "workspaces": [
    "gatsby-wordpress-theme-libre",
    "example"
  ]
}
